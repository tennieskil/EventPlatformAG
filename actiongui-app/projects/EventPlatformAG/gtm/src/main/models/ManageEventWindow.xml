                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="Accept_B" ns1:width="100%" ns1:height="auto"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="Reject_B" ns1:width="100%" ns1:height="auto"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
