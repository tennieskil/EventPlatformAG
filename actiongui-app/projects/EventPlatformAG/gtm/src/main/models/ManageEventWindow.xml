                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="Invite_B" ns1:width="100%" ns1:height="auto"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="Posts_L" ns1:width="100%" ns1:height="23px"/>
