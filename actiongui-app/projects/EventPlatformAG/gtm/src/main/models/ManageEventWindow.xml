                    <ns1:line-layout orientation="horizontal"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="100%">
                        <ns1:entry expand-ratio="1">
                            <ns1:line-layout orientation="horizontal"/>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="100%">
                                <ns1:entry>
                                    <ns1:table ns1:id="Table_Att_T" ns1:width="100%" ns1:height="100px">
                                        <ns1:column>
                                            <ns1:widget ns1:id="title" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                    </ns1:table>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="Add_B" ns1:width="100%" ns1:height="auto"/>
